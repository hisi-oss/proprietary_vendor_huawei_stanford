	</Mode>
	<Mode name="opticalzoomselfportrait" switcher="1">
		<PPAlgo name="hwaxymap" />
		<PPAlgo name="dualdepth" />
		<PPAlgo name="sr" />
		<PPAlgo name="davinci" />
		<PPAlgo name="refocus" />
